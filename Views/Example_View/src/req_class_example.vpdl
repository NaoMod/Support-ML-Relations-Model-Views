create view component_block as

select Spec.Requirement.*,
	   UML.Class.*,
	   Spec.Requirement join UML.Class  as relates_with

from 'http://requirements' as Spec,
	 'http://www.eclipse.org/uml2/5.0.0/UML' as UML,
	 
where "{Requirement.longName,Requirement.ID} <~> {Class.name,Class.qualifiedName}" for relates_with